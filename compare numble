#include <stdio.h>


int Max(int x, int y)
{
	if (x > y)
		return x;
	else
		return y;
}
int main()
{

	int num1 = 10;
	int num2 = 20;
	int max = 0;
	max = Max(num1, num2);
	printf("max= %d\n", max);
	return 0;


{
	int a = 10;
	int arr[] = { 1, 2, 3, 4, 5, 6 };// 4*6=24
	printf("%d\n", sizeof(a));//4
	printf("%d\n", sizeof(int));//4
	printf("%d\n", sizeof a);//4
	printf("%d\n", sizeof(arr));//计算数组大小，单位是字节

}
